{
    "name": "kujua-base",
    "version": "0.1.22-pre",
    "description": "Provide access to your SMS form data.",
    "long_description" : "Kujua is a ....",
    "url": "https://github.com/medic/kujua",
    "categories": ["cdc", "nepal"],
    "icons": {
        "16": "static/img/promo/icon_16.png",
        "48": "static/img/promo/icon_48.png",
        "96": "static/img/promo/icon_96.png",
        "128": "static/img/promo/icon_128.png"
    },
    "promo_images": {
        "small": "static/img/promo/promo_small.png"
    },
    "screenshots": [
        "static/img/promo/screenshot1.png"
    ],
    "settings_schema" : {
        "description": "Kujua Settings",
        "type": "object",
        "properties": {
            "google_analytics_code": {
                "type": "string"
            }
        }
    },
    "loglevel": "info",
    "load": "lib/app",
    "modules": ["lib"],
    "attachments": ["static", "docs"],
    "dust": {
        "templates": "templates"
    },
    "duality": {
        "base_template": "base.html"
    },
    "duality-contrib-loading": {
        "timeout": 300
    },
    "coffee-script": {
      "attachments": ["static/js"],
      "modules": []
    },
    "less": {
        "compress": true,
        "compile": ["static/css/app.less", "static/css/spreadsheet.less"],
        "remove_from_attachments": true
    },
    "node_module_folder" : "sentinel",
    "dependencies": {
        "modules": null,
        "properties": null,
        "attachments": null,
        "settings": null,
        "duality": ">=0.0.18",
        "duality-contrib-session": null,
        "dust": null,
        "duality-dust": null,
        "nodeunit": null,
        "nodeunit-testrunner": null,
        "coffee-script-precompiler": null,
        "less-precompiler": null,
        "bootstrap-fontawesome": null,
        "kanso-config": null,
        "kujua-branding": ">=0.0.2",
        "kujua-sms": null,
        "kujua-utils": null,
        "kujua-reporting": null,
        "jsDump": null,
        "git-info": null,
        "showdown": null,
        "couch-fakerequest": null,
        "users": null,
        "cookies": null,
        "moment": null,
        "kanso-gardener": null
    }
}

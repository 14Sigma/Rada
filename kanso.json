{
    "name": "kujua-base",
    "version": "0.1.23-pre",
    "description": "SMS data collection designed for rural environments",
    "long_description" : "Kujua is a web based SMS data collection and analytics tool focused on community health care workers in the developing world. We help large health organizations collect and analyze data sent from health care workers using ordinary mobile phones in remote and rural environments.",
    "url": "https://github.com/medic/kujua",
    "categories": ["health", "sms"],
    "icons": {
        "16": "static/img/promo/icon_16.png",
        "48": "static/img/promo/icon_48.png",
        "96": "static/img/promo/icon_96.png",
        "128": "static/img/promo/icon_128.png"
    },
    "promo_images": {
        "small": "static/img/promo/promo_small.png"
    },
    "screenshots": [
        "static/img/promo/screenshot1.png"
    ],
    "settings_schema" : {
        "description": "Kujua Settings",
        "type": "object",
        "properties": {
            "id_format": {
                "type": "string",
                "default": "111111"
            },
            "ohw_counseling_reminder_days": {
                "title": "OWH counseling reminder days",      
                "type": "array",
                "items": {        
                    "type": "object",        
                    "order": ["days", "group"],       
                    "required": ["days", "group"],        
                    "properties": {          
                        "days": {            
                            "type": "number",            
                            "title": "Days"          
                        },          
                        "group": {            
                            "type": "number",            
                            "title": "Group"          
                        }
                    }
                }
            },
            "ohw_counseling_reminder_lbw_days" : {
                "title": "ohw counseling reminder lbw days",
                "type": "array",
                "items": {
                    "type": "object",
                    "order": ["days", "group"],
                    "required": ["days", "group"],
                    "properties": {
                        "days": {
                            "type": "number",
                            "title": "Days"
                        },
                        "group": {
                            "type": "number",
                            "title": "Group"
                        }
                    }
                }
            },
            "ohw_miso_reminder_days" : {
                "title": "ohw miso reminder days",
                "type": "array",
                "items": {
                    "type": "number"
                }
            },
            "ohw_obsolete_reminders_days" : {
                "type": "number",
                "default": 21
            },
            "ohw_outcome_request_days" : {
                "title": "ohw outcome request days",
                "type": "array",
                "items": {
                    "type": "number"
                }
            },
            "ohw_reminder_schedule_days" : {
                "title": "ohw reminder schedule days",
                "type": "array",
                "items": {
                    "type": "object",
                    "order": ["days", "group"],
                    "required": ["days", "group"],
                    "properties": {
                        "days": {
                            "type": "number",
                            "title": "Days"
                        },
                        "group": {
                            "type": "number",
                            "title": "Group"
                        }
                    }
                }
            },
            "ohw_upcoming_delivery_days" : {
                "title": "ohw upcoming delivery days",
                "type": "array",
                "items": {
                    "type": "number"
                }
            }
        }
    },
    "loglevel": "info",
    "load": "lib/app",
    "modules": ["lib"],
    "attachments": ["static", "docs"],
    "dust": {
        "templates": "templates"
    },
    "duality": {
        "base_template": "base.html"
    },
    "duality-contrib-loading": {
        "timeout": 300
    },
    "coffee-script": {
      "attachments": ["static/js"],
      "modules": []
    },
    "less": {
        "compress": true,
        "compile": ["static/css/app.less", "static/css/spreadsheet.less"],
        "remove_from_attachments": true
    },
    "node_module_folder" : "sentinel",
    "dependencies": {
        "modules": null,
        "properties": null,
        "attachments": null,
        "settings": null,
        "duality": ">=0.0.18",
        "duality-contrib-session": null,
        "dust": null,
        "duality-dust": null,
        "nodeunit": null,
        "nodeunit-testrunner": null,
        "coffee-script-precompiler": null,
        "less-precompiler": null,
        "bootstrap-fontawesome": null,
        "kanso-config": null,
        "kujua-branding": ">=0.0.2",
        "kujua-sms": null,
        "kujua-utils": null,
        "kujua-reporting": null,
        "jsDump": null,
        "git-info": null,
        "showdown": null,
        "couch-fakerequest": null,
        "users": null,
        "cookies": null,
        "moment": null,
        "kanso-gardener": null
    }
}
